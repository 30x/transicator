{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "changeserver",
    "labels": {
        "component": "changeserver"
    },
		"annotations": {
			"publicPaths": "9000:/changeserver",
			"projectcalico.org/policy": "allow tcp"
		}
	},
	"spec": {
		"containers": [{
			"name": "changeserver",
			"image": "changeserver:latest",
			"imagePullPolicy": "Never",
			"ports": [{
				"hostPort": 9000,
				"containerPort": 9000
			}]
		}]
	}
}
