{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "apigeepostgres",
    "labels": {
        "component": "apigeepostgres"
    },
		"annotations": {
			"publicPaths": "5432:/apigeepostgres",
			"projectcalico.org/policy": "allow tcp"
		}
	},
	"spec": {
		"containers": [{
			"name": "apigeepostgres",
			"image": "apigeepostgres:latest",
			"imagePullPolicy": "Never",
			"ports": [{
				"hostPort": 5432,
				"containerPort": 5432
			}]
		}]
	}
}
