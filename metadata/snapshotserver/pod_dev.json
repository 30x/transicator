{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "snapshotserver",
    "labels": {
        "component": "snapshotserver"
    },
		"annotations": {
			"publicPaths": "9000:/snapshotserver",
			"projectcalico.org/policy": "allow tcp"
		}
	},
	"spec": {
		"containers": [{
			"name": "snapshotserver",
			"image": "snapshotserver:latest",
			"imagePullPolicy": "Never",
			"ports": [{
				"hostPort": 9000,
				"containerPort": 9000
			}]
		}]
	}
}
